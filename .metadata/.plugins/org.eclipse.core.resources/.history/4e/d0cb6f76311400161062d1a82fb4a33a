package org.synoreso.synoreso;

import org.apache.logging.log4j.LogManager;
import org.apache.logging.log4j.Logger;

/**
 * Main class of this project!
 * @author JF
 * @version 1.0
 */
public class SynoReso {
	/**
	 * Logger Log4j de la classe SynoReso 
	 * @see SynoReso
	 */
	private static final Logger logger = LogManager.getLogger(SynoReso.class.getName());
	
	public static void main(String[] args) {
		System.out.println("Hello World!");
	}

	/**
	 * @return the logger
	 */
	public static Logger getLogger() {
		return logger;
	}
}
